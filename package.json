    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/lodash": "^4.14.166",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.28",
    "@types/node-fetch": "^2.5.7",
    "chai": "^4.2.0",
